{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore":  "Warning",
        "System": "Warning"
      }
    }
  },
  "DatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "MorphicTest"
  },
  "MorphicSettings": {
    "ServerUrlPrefix": "http://localhost:5002"
  },
  "EmailSettings": {
    "Disable": "true",
    "SendgridSettings": {
      "ApiKey": ""
    }
  }
}
